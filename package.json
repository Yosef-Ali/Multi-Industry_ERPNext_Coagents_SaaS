{
  "name": "multi-industry-erpnext-coagents",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "check": "npm run check:gateway && npm run check:frontend",
    "check:gateway": "cd services/agent-gateway && npm run check",
    "check:frontend": "cd frontend/coagent && tsc --noEmit",
    "smoke": "cd services/agent-gateway && npm run smoke",
    "dev:gateway": "cd services/agent-gateway && npm run dev",
    "dev:frontend": "cd frontend/coagent && pnpm run dev",
    "build": "npm run build:gateway && npm run build:frontend",
    "build:gateway": "cd services/agent-gateway && npm run build",
    "build:frontend": "cd frontend/coagent && pnpm run build"
  },
  "dependencies": {
    "resumable-stream": "^2.2.4"
  }
}